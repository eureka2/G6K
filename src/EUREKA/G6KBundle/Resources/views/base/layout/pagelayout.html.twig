{% import "EUREKAG6KBundle:base/macros:form-grid.html.twig" as grid %}
{% import "EUREKAG6KBundle:base/macros:form-classic.html.twig" as classic %}
{% import "EUREKAG6KBundle:base/macros:form-inline.html.twig" as inline %}
{% import "EUREKAG6KBundle:base/macros:form-blockinfo.html.twig" as blockinfo %}
{% import "EUREKAG6KBundle:base/macros:form.html.twig" as form %}

{% block doctype %}<!DOCTYPE html>{% endblock %}
{% block html %}<!-- paulirish.com/2008/conditional-stylesheets-vs-css-hacks-answer-neither/ -->
<!--[if lt IE 7 ]> <html lang="fr" class="no-js ie ie6" dir="ltr"> <![endif]-->
<!--[if IE 7 ]>    <html lang="fr" class="no-js ie ie7" dir="ltr"> <![endif]-->
<!--[if IE 8 ]>    <html lang="fr" class="no-js ie ie8" dir="ltr"> <![endif]-->
<!--[if IE 9 ]>    <html lang="fr" class="no-js ie ie9" dir="ltr"> <![endif]-->
<!--[if (gt IE 9)|!(IE)]><!-->
<html xmlns:esi="http://www.edge-delivery.org/esi/1.0" lang="fr"><!--<![endif]-->
{% endblock %}
<head>
{%- if script == 1 -%}<noscript><meta http-equiv="refresh" content="0; url=?no-js=1" /></noscript>{% endif %}
{%- if script == 0 -%}
<script type="text/javascript">
<!--
if (/no-js=/.test(window.location.search)) {
	window.location.search = "";
}
-->
</script>
{% endif %}

<meta charset="utf-8" />
<title>{% block title %}{% endblock %}</title>
{% block metas %}
<meta http-equiv="Content-Type" content="text/html; charset=UTF-8" />      
<!--[if IE]>
<meta http-equiv="X-UA-Compatible" content="IE=Edge" />
<![endif]-->    
{% endblock %}

{% block styles %}
<link rel='stylesheet' type='text/css' href="{{ asset('bundles/eurekag6k/base/css/g6k.css') }}" />
{% for widget, props in widgets %}
{% for css in props.css %}
<link rel="stylesheet" type="text/css" href="{{ asset('bundles/eurekag6k/base/widgets/' ~ widget ~ '/' ~ css ) }}" />
{% endfor %}
{% endfor %}
{% endblock %}
{% block hscripts %}
<script type="text/javascript" src="{{ asset('bundles/eurekag6k/base/js/modernizr.min.js') }}"></script>
<!--[if lt IE 9]>
<script src="{{ asset('bundles/eurekag6k/base/js/html5shiv.min.js') }}"></script>
<script src="{{ asset('bundles/eurekag6k/base/js/respond.min.js') }}"></script>
<![endif]-->    
{% endblock %}
</head>
<body class="{% block bodyclass %}{% endblock %}">

{% block body %}
{% block content %}{% endblock %}

{% block scripts %}
{% block jquery %}
<!--[if lt IE 9]>
<script src="{{ asset('bundles/eurekag6k/base/js/jquery-1.11.1.min.js') }}"></script>
<![endif]-->
<!--[if gte IE 9]><!-->
<script type="text/javascript" src="{{ asset('bundles/eurekag6k/base/js/jquery-2.1.4.min.js') }}"></script>
<!--<![endif]-->
{% endblock %}
<script type="text/javascript" src="{{ asset('bundles/eurekag6k/base/js/jquery.toggleOption.js') }}"></script>
<script type="text/javascript" src="{{ asset('bundles/eurekag6k/base/js/jquery.swap-image.js') }}"></script>
<script type="text/javascript" src="{{ asset('bundles/eurekag6k/base/js/jquery.cookie.js') }}"></script>
<script type="text/javascript" src="{{ asset('bundles/eurekag6k/base/js/jquery.placeholder.min.js') }}"></script>
<script type="text/javascript" src="{{ asset('bundles/bazingajstranslation/js/translator.min.js') }}"></script>
<script type="text/javascript" src="{{ asset('js/translations/config.js') }}"></script>
<script type="text/javascript" src="{{ asset('js/translations/messages/' ~ app.request.locale ~ '.js') }}"></script>
<script type="text/javascript" src="{{ asset('bundles/eurekag6k/base/js/accounting.min.js') }}"></script>
<script type="text/javascript" src="{{ asset('bundles/eurekag6k/base/js/DefiantJS/defiant.min.js') }}"></script>
<script type="text/javascript" src="{{ asset('bundles/eurekag6k/base/js/JSONPath/jsonpath.js') }}"></script>
{% for widget, props in widgets %}
{% for js in props.js %}
<script type="text/javascript" src="{{ asset('bundles/eurekag6k/base/widgets/' ~ widget ~ '/' ~ js ) }}"></script>
{% endfor %}
{% endfor %}
<script type="text/javascript" src="{{ asset('bundles/eurekag6k/base/js/g6k.min.js') }}"></script>
<script type="text/javascript">
jQuery(document).ready(function(){
	{%- if not(ua.mobile) -%}
		$('.disposition-classic input.date, .disposition-grid input.date').each(function() {
			$(this).attr('data-widget', 'abDatepicker'); {# Temporarily #}
		});
	{% endif %}
	var g6k = new G6k({%- if step.dynamic or (step.simulator.dynamic and step.id == 0) -%}true{%- else -%}false{%- endif %}, {% if ua.mobile -%}true{%- else -%}false{%- endif -%}, "{{ app.request.locale }}");
	g6k.run();
	$(':input[data-widget]').each(function() {
		var widget = window[$(this).attr('data-widget')];
		var that = $(this);
		widget.call(null, that, function (value, text) {
			that.val(value);
			that.trigger('change');
		});
	});
});
</script>
{% endblock %}
{% endblock %}

</body>
</html>


